//REVISADO
model Usuario {
  id            String   @id
  habilitado    Boolean
  nombre        String
  apellido      String
  eliminado     Boolean
  primerIngreso Boolean
  usuarioRoles  UsuarioRole[]
  empresaId     String?   @unique
  empresa       Empresa? @relation("EmpresaUsuario", fields: [empresaId], references: [id])
}