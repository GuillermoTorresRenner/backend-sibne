model Planta {
  id        Int    @id @default(autoincrement())
  codigo    Int
  nombre    String
  activo    Boolean
  comunaId  Int
  empresaId String

  // Relaciones
  empresa   Empresa @relation(fields: [empresaId], references: [id])
  comuna    Comuna  @relation(fields: [comunaId], references: [id])
  transacciones Transaccion[]
  encuestaPlantas EncuestaPlanta[]
}
