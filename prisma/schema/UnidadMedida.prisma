//REVISADO
model UnidadMedida {
  id                Int    @id @default(autoincrement())
  codigoStr         String
  nombre            String
  tipoUnidad        String
  destinoId         Int?
  origenId          Int?
  visibleSelect     Boolean

  origen            UnidadMedida? @relation("Origen", fields: [origenId], references: [id])
  origenes          UnidadMedida[] @relation("Origen")
  destino           UnidadMedida? @relation("Destino", fields: [destinoId], references: [id])
  destinos          UnidadMedida[] @relation("Destino")
  energeticoUnidadMedida EnergeticoUnidadMedida[]
  factoresOrigen    FactorConversion[] @relation("UOrigen")
  factoresDestino   FactorConversion[] @relation("UDestino")
  transaccionIn     Transaccion[] @relation("UnidadMedidaIn")
  transaccionOut    Transaccion[] @relation("UnidadMedidaOut")
  transaccionNoAprovechada Transaccion[] @relation("UnidadMedidaNoAprovechada")
  transaccionCapInstalada  Transaccion[] @relation("UnidadMedidaCapInstalada")

  @@map("UnidadMedida")
}